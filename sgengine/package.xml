<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>sgengine</name>
  <version>1.0.0</version>
  <description>SpaceGrantEngine - ROS2 Utilities Package</description>
  <maintainer email="jcdavis@mines.edu">jcdavis</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_python</buildtool_depend>
  
  <depend>rclpy</depend>
  <depend>std_msgs</depend>
  <depend>depthai</depend>
  <depend>depth_image_proc</depend>
  <depend>numpy</depend>
  <depend>opencv-python</depend>
  <depend>uvloop</depend>
  <depend>flask</depend>
  <depend>scikit-learn</depend>
  <depend>rtabmap_ros</depend>
  <depend>geometry_msgs</depend>
  
  <depend>sgengine_messages</depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
